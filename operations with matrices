import numpy as np

A=np.array([[1,5],[2,5]])

B=np.array([[3,1],[7,4]])

A@B        #@ je za mnozenje matrica
#array([[38, 21],
#       [41, 22]])



C=np.array([[1,3,0],[2,1,4]])

D=np.array([[3],[6],[2]])

C@D
#array([[21],
#       [20]])



E=np.array([[1,1],[-1,2]])

F=np.array([[0,1,2],[2,0,3]])

E@F
#array([[ 2,  1,  5],
#       [ 4, -1,  4]])
